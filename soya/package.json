{
  "name": "soya",
  "version": "0.0.53",
  "description": "Hopefully hassle-free web front-end framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/traveloka/soya.git"
  },
  "keywords": [
    "webpack",
    "isomorphic",
    "universal",
    "front-end",
    "framework",
    "soya"
  ],
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/babel src/ --out-dir lib --source-maps --copy-files",
    "watch": "./node_modules/.bin/babel src/ --out-dir lib --source-maps --copy-files --watch",
    "watch-starter": "./node_modules/.bin/babel src/ --out-dir \"../soya-starter/node_modules/soya/lib\" --source-maps --copy-files --watch",
    "watch-starter-redux": "./node_modules/.bin/babel src/ --out-dir \"../soya-starter-redux/node_modules/soya/lib\" --source-maps --copy-files --watch",
    "watch-test-integration": "./node_modules/.bin/babel src/ --out-dir \"../test/integration/node_modules/soya/lib\" --source-maps --copy-files --watch"
  },
  "peerDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-flow-comments": "^1.0.9",
    "babel-plugin-remove-nonjs": "0.0.2",
    "babel-runtime": "^5.8.20",
    "css-loader": "^0.19.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "mime": "^1.3.4",
    "node-sass": "^3.10.0",
    "file-loader": "^0.8.4",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-addons-update": "^0.14.6",
    "sass-loader": "^4.0.2",
    "socket.io": "^1.4.8",
