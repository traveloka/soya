{
  "name": "soya",
  "version": "0.2.13",
  "description": "Hopefully hassle-free web front-end framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/traveloka/soya.git"
  },
  "keywords": [
    "webpack",
    "isomorphic",
    "universal",
    "front-end",
    "framework",
    "soya"
  ],
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/babel src/ --out-dir lib --source-maps --copy-files",
    "watch": "./node_modules/.bin/babel src/ --out-dir lib --source-maps --copy-files --watch",
    "watch-starter": "./node_modules/.bin/babel src/ --out-dir \"../soya-starter/node_modules/soya/lib\" --source-maps --copy-files --watch",
    "watch-starter-redux": "./node_modules/.bin/babel src/ --out-dir \"../soya-starter-redux/node_modules/soya/lib\" --source-maps --copy-files --watch",
    "watch-test-integration": "./node_modules/.bin/babel src/ --out-dir \"../test/integration/node_modules/soya/lib\" --source-maps --copy-files --watch"
  },
  "peerDependencies": {
    "extract-text-webpack-plugin": "^3.0.0",
    "node-sass": "^4.5.3",
    "react": "^15.6.1",
    "react-addons-update": "^15.6.0",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.4.5",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.18.0"
  },
  "dependencies": {
    "babel-loader": "^7.1.2",
    "config": "^1.26.2",
    "css-loader": "^0.28.7",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-entities": "^1.2.0",
    "js-yaml": "^3.4.3",
    "prop-types": "^15.6.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "source-map-support": "^0.4.18",
    "style-loader": "^0.18.2",
    "styled-modules": "^0.2.3",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ],
    "plugins": [
      "transform-export-extensions",
      "transform-object-assign"
    ]
  }
}
