    "style-loader": "^0.12.4",
    "webpack": "^1.12.2"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-eslint": "^4.1.1",
    "babel-plugin-flow-comments": "^1.0.9",
    "babel-runtime": "^5.8.20",
    "express": "^4.13.3"
  },
  "dependencies": {
    "crypto-js": "^3.1.8",
    "html-entities": "^1.2.0",
    "js-yaml": "^3.4.3",
    "memory-fs": "^0.2.0",
    "rimraf": "^2.4.3"
  }
